
<launch>
 <!-- 开启仿真节点-->
 <include file="$(find akm_control)/launch/akm_control.launch" >
   <arg name="gazebo_navigation" default="true"/>
 </include>
 <!-- 设置需要用于导航的地图  -->
 <arg name="map_file" default="$(find akm_control)/map/map.yaml"/>
 <node name="map_server_for_test" pkg="map_server" type="map_server" args="$(arg map_file)" output="screen" >
 </node>
 <!-- 开启用于导航的自适应蒙特卡洛定位amcl-->
 <include file="$(find akm_control)/launch/include/amcl.launch" />
 <!-- MarkerArray功能节点 -->
 <node name='send_mark' pkg="akm_control" type="send_mark.py">
 </node>
</launch>




